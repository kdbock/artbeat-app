# Local Environment Variables (DO NOT COMMIT THIS FILE)
# Copy this to .env.local and fill in your actual values

# Environment (development, staging, production)
export ENVIRONMENT=development

# Google Maps API Key
# Get from: https://console.cloud.google.com/apis/credentials
export GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Stripe Publishable Key
# Get from: https://dashboard.stripe.com/apikeys
# Use pk_test_* for development, pk_live_* for production
export STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key_here

# Firebase Region (optional)
export FIREBASE_REGION=us-central1

# API Base URL (optional)
export API_BASE_URL=https://api.artbeat.app
